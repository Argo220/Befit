
@model BeFit.Models.TrainingSession
@{ ViewData["Title"] = "Szczegóły"; }
<h2>Szczegóły</h2>
<dl class="row">
    <dt class="col-sm-3">Początek</dt>
    <dd class="col-sm-9">@Model.StartTime</dd>
    <dt class="col-sm-3">Koniec</dt>
    <dd class="col-sm-9">@Model.EndTime</dd>

</dl>
<p>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Edytuj</a>
    <a class="btn btn-secondary" asp-action="Index">Powrót</a>
</p>
